<div style='color: black;'>
  <h1>
    This is the New Picture page
  </h1>
  <%= yield %>
</div>

<div>
  <%= form_for @picture do |f|%>
    <%= f.label :title, "Title"%>
    <%= f.text_field :title %>
    <%= f.label :description, "Description" %>
    <%= f.text_field :description %>
    <%= f.text_field :image %>
    <%= f.submit %>
  <% end %>
</div>

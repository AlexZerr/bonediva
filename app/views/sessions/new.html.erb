
<h1>Login</h1>

<%= form_tag sessions_path do %>
<div>
  <%= label_tag :email %>
  <%= text_field_tag :email, nil, required: true %>
</div>

<div>
  <%= label_tag :password %>
  <%= password_field_tag :password, nil, required: true %>
</div>
  <p class="button"><%= submit_tag "Log In" %>
<% end %>

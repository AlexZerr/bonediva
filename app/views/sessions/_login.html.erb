<div class="login_position">
  <h1>Login</h2>
</div>

<%= form_tag sessions_path do %>
<div>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
</div>

<div>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</div>
  <p class="button"><%= submit_tag "Log In" %>
<% end %>
<% yield %>
